// write a program to add sum of an array element
#include<iostream>
using namespace std;
int main(){
	int arr[10],sum=0,n;
	cout<<"enter number of element of an array"<<endl;
	cin>>n;
	cout<<"elements are :"<<endl;
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	for(int i=0;i<n;i++){
		sum=sum+arr[i];
	}
	cout<<"sum of an array element is :"<<sum<<endl;
	return 0;
}
