// Write a program to convert uppercase alphabet to lowecase

#include<iostream>
#include<string.h>
using namespace std;
int main(){
	string s1;
	cout<<"enter a string "<<endl;
	cin>>s1;
	int n=s1.size();
	for(int i=0;i<n;i++){
	if(s1[i]>=65 &&s1[i]<=92){
		s1[i]=s1[i]+32;
	}
}
cout<<s1;
return 0;
}
