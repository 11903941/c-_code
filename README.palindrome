
// write a program to check a number is palindrome or not
#include<iostream>
using namespace std;
int main(){
	int num,org,rem,rev=0;
	cout<<"enter number"<<endl;
	cin>>num;
	org=num;
	while(org!=0){
		rem=org%10;//838
		rev=rev*10+rem;
		org=org/10;
		
	}
	if(num==rev){
		cout<<"number is palindrome"<<endl;
	}
	else{
		cout<<"number is not palindrome"<<endl;
	}
	return 0;
}
