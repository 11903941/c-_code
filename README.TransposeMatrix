//rint a Transpose of a matrix
#include<iostream>
using namespace std;
int main(){
	int arr[10][10];
	int m,n,i,j;
	cin>>m>>n;
	cout<<"enter a matrix :"<<endl;
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++){
			cin>>arr[i][j];
		}
		cout<<endl;
	}
	
	cout<<"A matrix is :"<<endl;
	for(i=0;i<m;i++)
	{
		for(j=0;j<n;j++){
			cout<<arr[i][j]<<ends;
		}
		cout<<endl;
	}
	
	cout<<"Transpose of a matrix is :"<<endl;
	for(i=0;i<n;i++)
	{
		for(j=0;j<m;j++){
			cout<<arr[j][i]<<ends;
		}
		cout<<endl;
	}
}
