//Write a program to print student record using structure

#include<iostream>
using namespace std;
int main(){
	int i,n;
	cout<<"enter the number of student "<<endl;
	cin>>n;
	struct student {
		int rollNo;
		string name;
		float avgmarks;
		char grade;
		int regNo;	
	};
	struct student s;
	for(int i=0;i<n;i++){
	cin>>s.rollNo>>s.name>>s.avgmarks>>s.grade>>s.regNo;
	cout<<endl<<endl;
	cout<<s.rollNo<<ends<<s.name<<ends<<s.avgmarks<<ends<<s.grade<<ends<<s.regNo<<endl<<endl;
}
	return 0;
}
