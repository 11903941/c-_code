// Write a program to cyclically rotate an array by one
#include<iostream>
using namespace std;
int main(){
	int i;
	int arr[]={2,5,7,1,9,8,0,6};
	int n=sizeof(arr)/sizeof(arr[0]);
	int temp=arr[n-1];
	for( i=n-1;i>0;i--){
		arr[i]=arr[i-1]; 	
	}
	arr[0]=temp;
	for(i=0;i<n;i++){
	cout<<arr[i]<<ends;
	}
	return 0;
}
