// Write a program to print a string is palidrome or not

#include<iostream>
#include<string.h>
using namespace std;
int main(){
	string s1;
	cout<<"enter a string"<<endl;
	cin>>s1;
	int i,count=0,n=s1.size();
    for(i=0;i<n/2;i++,n--){
    if(s1[i]==s1[n-1]){
    	count++;
	}
}
if(count==n/2){
	cout<<"String is a palidrome"<<endl;
}
else{
	cout<<"String is not a palidrome"<<endl;
}
	return 0;
}

